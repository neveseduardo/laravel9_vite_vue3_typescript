{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "resources/ts/*"
      ]
    },
  },
  "include": [
    "resources/ts/**/*.ts",
    "resources/ts/**/*.d.ts",
    "resources/ts/**/*.tsx",
    "resources/ts/**/*.vue"
  ],
  "references": [
  ]
}